#include <inttypes.h>

#ifndef REG_STRUCTS_H_
#define REG_STRUCTS_H_

typedef struct {
	uint32_t MODER;
	uint32_t OTYPER;
	uint32_t OSPEEDR;
	uint32_t PUPDR;
	uint32_t IDR;
	uint32_t ODR;
	uint32_t BSRR;
} GPIOx;

#endif /* REG_STRUCTS_H_ */
