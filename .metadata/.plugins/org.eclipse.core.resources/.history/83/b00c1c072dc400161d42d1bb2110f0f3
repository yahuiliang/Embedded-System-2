#define RCC_AHB1ENR 0x40023830
#define GPIOBEN 1 << 1
#define GPIOCEN 1 << 2
#define GPIOB_BASE 0x40020400
#define GPIOC_BASE 0x40020800
#define MODER 0
#define IDR 4
#define ODR 5
#define Px0 0
#define Px1 1
#define Px2 2
#define Px3 3

#define SET 1
#define OUTPUT 1
#define RS_CLR 1 << (0 + 16)
#define RW_CLR 1 << (1 + 16)

void lcd_init();
void lcd_clear();
void lcd_home();
void lcd_set_position();
void lcd_print_string();
void lcd_print_num();
